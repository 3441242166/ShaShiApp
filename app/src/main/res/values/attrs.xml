<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="hint_content" format="string" />
    <attr name="item_content" format="string" />

    <attr name="item_icon" format="integer" />
    <attr name="left_icon" format="integer" />
    <attr name="right_icon" format="integer" />
    <attr name="right_type" format="integer" />
    <attr name="input_type" format="integer" />

    <attr name="icon_show" format="boolean" />
    <attr name="second_title_show" format="boolean" />

    <declare-styleable name="PostChooseLayout">
        <attr name="title"/>
        <attr name="hint_content"/>
        <attr name="item_content"/>
    </declare-styleable>

    <declare-styleable name="PostEditLayout">
        <attr name="title"/>
        <attr name="hint_content"/>
        <attr name="item_content"/>
        <attr name="input_type"/>
    </declare-styleable>

    <declare-styleable name="PostEditPlusLayout">
        <attr name="title"/>
        <attr name="hint_content"/>
        <attr name="item_content"/>
    </declare-styleable>

    <declare-styleable name="PostLocationLayout">
        <attr name="title"/>
        <attr name="hint_content"/>
        <attr name="item_content" />
    </declare-styleable>

    <declare-styleable name="SettingNormalLayout">
        <attr name="title"/>
        <attr name="second_title"/>
        <attr name="left_icon" />
        <attr name="icon_show"/>
        <attr name="second_title_show" />
    </declare-styleable>

    <declare-styleable name="SettingSwitchLayout">
        <attr name="title"/>
        <attr name="right_type"/>
    </declare-styleable>

    <declare-styleable name="SettingChooseLayout">
        <attr name="title"/>
        <attr name="right_type"/>
    </declare-styleable>

    <declare-styleable name="MessageLayout">
        <attr name="title"/>
        <attr name="item_content"/>
        <attr name="item_icon"/>
    </declare-styleable>

    <declare-styleable name="LoginEditText">
        <attr name="hint_content"/>
        <attr name="left_icon"/>
        <attr name="right_icon"/>
        <attr name="input_type"/>
    </declare-styleable>

    <declare-styleable name="ImageButton">
        <attr name="item_content"/>
        <attr name="item_icon"/>
    </declare-styleable>

</resources>